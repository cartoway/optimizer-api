<body data-page="result">
<div id="map-container">
    <div id="sidebar">
        <div class="block">
            <h1><%= I18n.t('views.result.title') %></h1>
        </div>
        <div class="block form">
            <label for="job-id">Job id</label>
            <div>
                <input placeholder="Job id" id="job-id"/>
            </div>
            <button id="get-job"><%= I18n.t('views.result.show_result') %></button>
        </div>
        <div id="checkbox-container"></div>
        <div id="select-container"></div>
        <div class="block">
            <button id="back"><%= I18n.t('views.result.back') %></button>
        </div>
    </div>
    <div id="map"></div>
</div>
</body>
</html>
